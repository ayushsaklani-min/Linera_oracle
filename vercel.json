{
  "version": 2,
  "framework": null,
  "installCommand": "cd frontend-v2 && npm install",
  "buildCommand": "cd frontend-v2 && npm run build",
  "outputDirectory": "frontend-v2/dist",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}




